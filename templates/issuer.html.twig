{#
/**
 * @file issuer.html.twig
 * Default theme implementation to present Issuer data.
 *
 * This template is used when viewing Issuer pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_issuer()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('issuer') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
